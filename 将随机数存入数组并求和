#include<stdio.h>  
#include<stdlib.h>
#include<time.h>

int main() {  	
int arr[10]={0};
int len=sizeof(arr)/sizeof(arr[0]);
srand(time(NULL));
for(int i=0;i<len;i++){

int num=rand()%100+1;
 arr[i]=num;

}

int sum=0;


for(int a=0;a<len;a++){
	printf("%d\n",arr[a]);

sum=sum+arr[a];
	
}
printf("%d\n",sum);
return 0;
}


