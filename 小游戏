#include<stdio.h>  
#include<stdlib.h>
#include<time.h>
#include<conio.h>
int main() {  	

srand(time(NULL));
printf("Press any key to  ");
getch();
int num1=rand()%6+1;
printf(" your number is %d\n",num1);
int num2=rand()%6+1;
printf("the computer's number is %d\n",num2);
if(num1>num2){
	printf("you win");

}
else if(num1<num2){
	printf("you lose");
}
else{
	printf("To finish at the same time");
}


return 0;
}

