#include<stdio.h>
int main(){
	int arr1[]={12,23,4,4,5,5,54};
	int arr2[]={1,1,3,2,4,23,4,234,234};
	int arr3[]={3,23,5,34,534,6,36,36,436,45,654,645};
	
	
	int len1=sizeof(arr1)/sizeof(int);
	int len2=sizeof(arr2)/sizeof(int);
	int len3=sizeof(arr3)/sizeof(int);
	
	int arr4[]={len1,len2,len3};
	int* arr[]={arr1,arr2,arr3};
    for(int i=0;i<3;i++){
    	
    	for(int j=0;j<arr4[i];j++){
    		
    		printf("%d ",arr[i][j]);
		}
		printf("\n");
	}	



}
