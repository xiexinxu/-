#include<stdio.h>
#include<string.h>
int main(){
	printf("give the number group's length: ");
	int n=0;
	scanf("%d",&n);
getchar(); // 吸收换行符
    printf("give number group's word: ");
	 char str[n + 1];// 加1为了存储字符串的结束符 '\0'
fgets(str, sizeof(str), stdin);
 str[strcspn(str, "\n")] = 0;  
char* words[n];
int wordcount=0;
  char* p = strtok(str, " ");
int count[n];
memset(count, 0, sizeof(count));
while(p!=NULL){
	words[wordcount]=p;
	wordcount++;
	  p = strtok(NULL, " ");
}
for(int i=0;i<wordcount;i++){
	for(int j=0;j<i;j++){
		if(strcmp(words[i],words[j])==0){
			count[j]++;
			
			break;
		}
	}
	
	count[i]++;
	
}

    for (int i = 0; i < wordcount; i++) {  
        if (count[i] > 1) {  
            printf("number: %s, number's count: %d\n", words[i], count[i]);  
        }  
    }  

    return 0;  
}  




